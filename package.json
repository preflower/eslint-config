{
  "name": "eslint-config-ted",
  "version": "3.0.2",
  "description": "Ted's Eslint Rules",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "lint": "npx eslint --ext .js,.json,.vue --fix .",
    "ci:version": "pnpm changeset version && pnpm install --lockfile-only",
    "ci:publish": "pnpm changeset publish"
  },
  "keywords": [
    "eslint",
    "vue",
    "react",
    "typescript",
    "config"
  ],
  "repository": "https://github.com/preflower/eslint-config",
  "author": "Ted Lin <preflower.lin@gmail.com> (https://github.com/preflower)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/preflower/eslint-config/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "eslint": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsonc": "^2.16.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-vue": "^9.27.0",
    "jsonc-eslint-parser": "^2.4.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.8",
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@types/react": "^18.3.3",
    "eslint": "^8.57.0",
    "husky": "^8.0.3",
    "react": "^18.3.1",
    "typescript": "^5.5.4",
    "vue": "^3.4.33"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "typescript.js",
    "vue.js",
    "react.js"
  ]
}
