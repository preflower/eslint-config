{
  "name": "eslint-config-ted",
  "version": "2.0.1",
  "description": "Ted's Eslint Rules",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "lint": "npx eslint --ext .js,.json,.vue --fix .",
    "ci:version": "pnpm changeset version && pnpm install --lockfile-only",
    "ci:publish": "pnpm changeset publish"
  },
  "keywords": [
    "eslint",
    "vue",
    "react",
    "typescript",
    "config"
  ],
  "repository": "https://github.com/preflower/eslint-config",
  "author": "Ted Lin <preflower.lin@gmail.com> (https://github.com/preflower)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/preflower/eslint-config/issues"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "peerDependencies": {
    "eslint": "*",
    "typescript": "*"
  },
  "dependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^36.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsonc": "^2.9.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^8.0.3",
    "jsonc-eslint-parser": "^2.3.0",
    "vue-eslint-parser": "^9.3.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "eslint": "^8.45.0",
    "typescript": "^5.1.6",
    "vue": "^3.3.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "typescript.js",
    "vue.js",
    "react.js"
  ]
}
